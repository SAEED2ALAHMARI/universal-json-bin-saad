{
  "author": "Vipin Tanna",
  "homepage": "https://universal-json-schema.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vip-git/universal-json-schema.git"
  },
  "bugs": {
    "url": "https://github.com/vip-git/universal-json-schema/issues"
  },
  "scripts": {
    "postinstall": "cross-env FRAMEWORK_NAME=$FRAMEWORK_NAME UI_FRAMEWORK_NAME=$UI_FRAMEWORK_NAME node scripts/generator/index.js",
    "build": "stencil build --docs",
    "build-storybook": "build-storybook",
    "dev": "stencil build --watch --serve",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "serve": "serve www -p 5000",
    "start": "run-p dev storybook",
    "storybook": "start-storybook -p 6006 -s dist/",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "nanoid": "3.1.23",
    "xstate": "4.19.2"
  },
  "description": "Ionic UI implementation of stencil-jsonschema-form with UI and XHR Schema.",
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@stencil/core": "1.17.2",
    "@stencil/eslint-plugin": "0.3.1",
    "@storybook/addon-essentials": "6.0.0-rc.26",
    "@storybook/web-components": "6.0.0-rc.26",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "@typescript-eslint/parser": "3.8.0",
    "babel-loader": "8.1.0",
    "copy-webpack-plugin": "6.0.3",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.5",
    "jest": "26.2.2",
    "lit-html": "1.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "webpack": "4.44.1",
    "write-file-webpack-plugin": "4.5.1"
  },
  "license": "ISC",
  "main": "index.js",
  "name": "stencil-jsonschema-form-ionic-ui",
  "peerDependencies": {
    "immutability-helper": "2.6.5 || ^3.0.0"
  },
  "version": "1.0.0-beta.0",
  "keywords": []
